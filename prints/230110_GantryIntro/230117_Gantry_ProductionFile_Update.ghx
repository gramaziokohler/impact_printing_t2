                    <item name="CodeInput" type_name="gh_string" type_code="10">"""
Publishes messages to a ROS topic

COMPAS FAB v0.25.0
"""

__author__ = "kchadha"
__version__ = "2022.12.17"

import Rhino.Geometry as rg
import time
import json
from ghpythonlib.componentbase import executingcomponent as component
from roslibpy import Topic
from scriptcontext import sticky as st

from compas_fab.backends.ros.messages import ROSmsg
from compas_fab.ghpython.components import create_id



class ROSTopicPublish(component):
    def RunScript(self, ros_client, send_geometry, start_button, go_to_position, shoot):
        ### designed waypoints
        topic1_name = '/design_waypoints'
        topic1_type = 'impact_printing_msgs/WaypointStates'
        
        ###digital start button for the tool
        topic2_name = '/controllino/digital_start_button'
        topic2_type = 'std_msgs/Bool'
        
        ###go to position
        topic3_name = '/next_waypoint'
        topic3_type = 'std_msgs/Bool'
        
        ##shoot 
        topic4_name = '/controllino/digital_shoot_button'
        topic4_type = 'std_msgs/Bool'
        
#        
#        
#        if not topic1_name or topic2_name or topic3_name or topic4_name :
#            raise ValueError('Please specify the name of the topic')
#        if not topic1_type or topic2_type or topic3_type or topic4_type:
#            raise ValueError('Please specify the type of the topic')
            
        key1 = create_id(self, 'topic1')
        key2 = create_id(self, 'topic2')
        key3= create_id(self, 'topic3')
        key4= create_id(self, 'topic4')
        
        topic1 = st.get(key1, None)
        topic2 = st.get(key2, None)
        topic3 = st.get(key3, None)
        topic4 = st.get(key4, None)

        if ros_client and ros_client.is_connected:
            if not topic1 or topic1.ros != ros_client:
                topic1 = Topic(ros_client, topic1_name, topic1_type, reconnect_on_close=False)
                topic2 = Topic(ros_client, topic2_name, topic2_type, reconnect_on_close=False)
                topic3 = Topic(ros_client, topic3_name, topic3_type, reconnect_on_close=False)
                topic4 = Topic(ros_client, topic4_name, topic4_type, reconnect_on_close=False)
                topic1.advertise()
                time.sleep(0.2)

                st[key1] = topic1
                st[key2] = topic2
                st[key3] = topic3
                st[key4] = topic4

        self.is_advertised_1 = topic1 and topic1.is_advertised
        self.is_advertised_2 = topic2 and topic2.is_advertised
        self.is_advertised_3 = topic3 and topic3.is_advertised
        self.is_advertised_4 = topic4 and topic4.is_advertised
        
###designed waypoints
        if send_geometry :
            msg1 = send_geometry
#            print type(msg1)
            print msg1
            msg1 = str(send_geometry)
            msg1 = ROSmsg.parse(msg1, topic1_type)
            topic1.publish(msg1.msg)
            self.Message = 'Message published'
        else:
            if self.is_advertised_1:
                self.Message = 'Topic1 advertised'
###start digital button for the tool
        if start_button:
            msg2 = json.dumps({'data': True})
        else:
            msg2 = json.dumps({'data': False})
        msg2 = ROSmsg.parse(msg2, topic2_type)
        topic2.publish(msg2.msg)
        time.sleep(0.1)
        self.Message = 'Message published'
        if self.is_advertised_2:
            self.Message = 'Topic2 advertised'
        

###go to next waypoint
        if go_to_position:
            msg3 = json.dumps({'data': True})
            msg3 = ROSmsg.parse(msg3, topic3_type)
            topic3.publish(msg3.msg)
            time.sleep(0.1)
            self.Message = 'Message published'
        else:
            msg3 = json.dumps({'data': False})
            if self.is_advertised_3:
                self.Message = 'Topic3 advertised'
                
### Shoot !!
        if shoot:
            msg4 = json.dumps({'data': True})
            msg4 = ROSmsg.parse(msg4, topic4_type)
            topic4.publish(msg4.msg)
            time.sleep(0.1)
            self.Message = 'Message published'
        else:
            msg4 = json.dumps({'data': False})
            if self.is_advertised_4:
                self.Message = 'Topic4 advertised'

                return(topic4,self.is_advertised_4)
        return (self.is_advertised_1,self.is_advertised_2, self.is_advertised_3, self.is_advertised_4)</item>
